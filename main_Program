int main(){
  long width;
  long height;
  long size;
  FILE* file;
  
  file = fopen(image, "rb");
  if(file==NULL) {
    printf("Couldn't open the file"); 
  }
  else {
    fseek(file, 2, SEEK_SET);
    fread(&size, 4, 1, file);
    fseek(file, 12, SEEK_CUR);
    fread(&width, 4, 1, file);
    fread(&height, 4, 1, file);
    }
  printf("The final width and height are: %l and %l.\n", width, height);
  printf("The total size is: %l.\n", size);
return 0;
}
